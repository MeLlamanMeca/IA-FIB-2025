###  http://www.rico-rico.com/ontologia#Menu
:Menu rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( :Menu
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasDessert ;
                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasFirstCourse ;
                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasMainCourse ;
                                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ;
      rdfs:label "Menú" .


